			<div class="large-4 medium-4 columns">
				<div id="country_geoip_opens_table_loading"></div> 
				<div id="country_geoip_opens_table"></div>
			</div>
			<div class="large-8 medium-8 columns">
				<div id="country_geoip_opens_map_loading"></div> 
