			</div>
		</div>
	<!-- /tmpl_if -->

	<h3>
		Message Opens Over Time
	</h3> 
		<div class="row">
			<div class="small-12 columns">
				<div id="over_time_opens_graph_loading"></div> 
				<div id="over_time_opens_graph"></div> 
			</div>
		</div>
