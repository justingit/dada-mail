	<div id="user_agent_opens_chart"   ></div>

	<div class="clearfix">
		<ul class="button-group right">
			<li>
			<form action="<!-- tmpl_var PluginURL -->" method="post"> 
				<input type="hidden" name="prm"    value="export_subscribers" > 
